{
    "parser": "babel-eslint",
    "extends": ["airbnb", "plugin:flowtype/recommended"]
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "flowtype",
    ],
    "rules": {
      "semi": ["error", "never"],
      "class-method-use-this": 0,
      "react/jsx-filename-extension": 0,
    },
}
